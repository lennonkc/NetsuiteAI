    Deposit anchor:  "Deposit Date“ < 现在时间月份 ? “Past Due” : "<Current Month name, like Mar., Feb. Jan.)>” 
    Deposit $ Due:  "Paid" > 0 ? 0 : <“Deposit % Due“ * “Balance”>
}
"Prepay":{
  Prepay Date : "Estimated Ready Date / ERD“
  Prepay % Due: “Prepay_H“
